{
    "windsurf.marketplaceExtensionGalleryServiceURL": "https://marketplace.visualstudio.com/_apis/public/gallery",
    "windsurf.marketplaceGalleryItemURL": "https://marketplace.visualstudio.com/items",
    "files.autoSave": "afterDelay",
    "terminal.integrated.defaultProfile.windows": "PowerShell5",
    "terminal.integrated.profiles.windows": {
        "PowerShell5": {
            "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe", 
            "overrideName": true,
            "icon": "terminal-powershell",
            "color": "terminal.ansiBlue"
        },
        "PowerShell7": {
            "path": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
            "overrideName": true,
            "icon": "terminal-powershell",
            "color": "terminal.ansiGreen"
        },
        "Command Prompt": null,
        "Git Bash": null,
        "Azure Cloud Shell": null,
        "Azure Cloud Shell (PowerShell)": null,
        "Azure Cloud Shell (Bash)": null,
        "JavaScript Debug Terminal": null,
        "PowerShell": null
    },

    "terminal.integrated.tabs.enabled": true,
    "terminal.integrated.defaultLocation": "view",
    "terminal.integrated.tabs.showActiveTerminal": "always",
    "workbench.panel.opensMaximized": "always",
    "terminal.integrated.enablePersistentSessions": false,
    "powershell.promptToUpdatePowerShell": false,
    "explorer.confirmDelete": false,
    "vsicons.dontShowNewVersionMessage": true,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.colorCustomizations": {
        "terminal.foreground": "#00fffc",
        "terminal.background": "#0c0a20",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#0f15d8",
        "terminal.ansiCyan": "#00fffc",
        "terminal.ansiGreen": "#5ffa74",
        "terminal.ansiMagenta": "#f924e7",
        "terminal.ansiRed": "#ff3045",
        "terminal.ansiWhite": "#c7c7c7",
        "terminal.ansiYellow": "#fffc7e",
        "terminal.ansiBrightBlack": "#686868",
        "terminal.ansiBrightBlue": "#3c40cb",
        "terminal.ansiBrightCyan": "#88fffe",
        "terminal.ansiBrightGreen": "#75ff88",
        "terminal.ansiBrightMagenta": "#f15be5",
        "terminal.ansiBrightRed": "#ff0019",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiBrightYellow": "#fffd96",
        "terminal.selectionBackground": "#0013ff",
        "terminalCursor.background": "#8c8c8c",
        "terminalCursor.foreground": "#c7c7c7"
    },
    "material-icon-theme.folders.color": "#00fffc",
    "material-icon-theme.files.color": "#00fffc",
    "material-icon-theme.rootFolders.color": "#00fffc",
    "material-icon-theme.hidesExplorerArrows": false,
    "material-icon-theme.saturation": 1,
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.activeIconPack": "vue",
    "material-icon-theme.rootFolders.associations": {},
    "material-icon-theme.folders.associations": {
        "PKG": "Command",
        "DEV": "Admin",
        "Assets": "",
        "Config": "",
        "Frontend": "",
        "Lib": "",
        "it": "",
        ".vscode": "",
        "_Tools": ""
    },
    "material-icon-theme.files.associations": {
        "Detection.ps1": "powershell",
        "Invoke-AppDeployToolkit.ps1": "powershell",
        "Invoke-AppDeployToolkit.exe": "exe"
    },
    "workbench.colorTheme": "Neon City"
}